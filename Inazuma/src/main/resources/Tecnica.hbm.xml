<hibernate-mapping>
<class name="com.example.Inazuma.Tecnica" table ="Tecnica">
<meta attribute="class-description">
Clase que almacena información de técnicas.
</meta>
<id name="idTecnica" type="int" column="idTecnica">
<generator class="native"/>
</id>
<property name="nombre" column="Nombre" type="string"/>
<property name="tipo" column="Tipo" type="string"/>
  <set name="jugadores" table="Jugador_Tecnica" inverse="true">
            <key column="idTecnica"/>
            <many-to-many class="com.example.Inazuma.Jugador" column="idJugador"/>
        </set>

    </class>
</hibernate-mapping>